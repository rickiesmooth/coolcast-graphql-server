{
  "name": "cc-api",
  "alias": "cc-api",
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@now/node@canary",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "/src/index.ts"
    }
  ]
}