{
  "name": "cc-api",
  "alias": "cc-api",
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "src/index.ts",
      "use": "node-builder-latest"
    },
    {
      "src": "src/schema.graphql",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/*",
      "dest": "/src/index.ts"
    }
  ]
}