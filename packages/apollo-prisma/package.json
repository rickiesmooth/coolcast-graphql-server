{
    "name": "@coolcast/apollo-prisma",
    "version": "0.0.3",
    "private": false,
    "scripts": {
        "start": "node dist/index.js",
        "build": "ncc build index.ts --no-minify --no-source-map",
        "generate-prisma-client": "cd src/prisma && prisma generate",
        "prisma-deploy": "cd prisma && prisma deploy",
        "generate": "graphqlgen"
    },
    "dependencies": {
        "@zeit/ncc": "^0.5.5",
        "bcrypt": "^3.0.2",
        "jsonwebtoken": "^8.4.0",
        "prisma": "^1.22.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/jsonwebtoken": "^8.3.0",
        "graphqlgen": "^0.4.0"
    }
}